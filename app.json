{
  "expo": {
    "name": "Frö",
    "slug": "fro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#6366f1"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "name": "Frö",
      "shortName": "Frö",
      "description": "Trygg innkrysning for barnehage",
      "themeColor": "#6366f1",
      "backgroundColor": "#1e1b4b",
      "display": "standalone",
      "display_override": ["standalone", "fullscreen", "minimal-ui"],
      "startUrl": "/",
      "scope": "/",
      "orientation": "portrait",
      "dir": "ltr",
      "lang": "no",
      "apple": {
        "appleTouchIcon": "./assets/images/icon.png",
        "appleStatusBarStyle": "black-translucent"
      },
      "manifest": {
        "icons": [
          {
            "src": "./assets/images/icon.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any maskable"
          },
          {
            "src": "./assets/images/icon.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any maskable"
          }
        ],
        "shortcuts": [
          {
            "name": "Admin Panel",
            "short_name": "Admin",
            "description": "Gå til admin panel",
            "url": "/admin",
            "icons": [
              {
                "src": "./assets/images/icon.png",
                "sizes": "192x192"
              }
            ]
          }
        ],
        "categories": ["productivity", "education"],
        "screenshots": [],
        "prefer_related_applications": false
      }
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1e1b4b",
          "dark": {
            "backgroundColor": "#1e1b4b"
          }
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "79d659c5-7d6f-447d-9ae0-4d35f8787c5a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/79d659c5-7d6f-447d-9ae0-4d35f8787c5a"
    }
